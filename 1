class author{
  constructor(name,email,gender){
this.name=name 
this.email=email 
this.gender=gender
  }
}  get name(){
  this._name
} get email(){
  this._email
} get gender(){
  this._gender
}  
set (name){
  this._name=name
}
set email(email){
  this._email = email
}
set gender(gender){
  this._gender = gender
  toString(){
    return "Yours email" email
  }


  class Book{
    constructor(title, author, price, quantity){
      this.title = title
      this.author = author
      this.price = price
      this.quantity = quantity
    }
      get title(){
          return this._title
      }
    get price(){
      return `this._price
    }
    get quantity(){
      return `this._quantity
    }
    get author(){
      return this._author
    }
    set title(name){
      this._title = name;
    }
    set author(name){
      this._author = name;
    }
    set price(value){
      this._price = value;
    }
    set quantity(value){
      this._quantity = value;
    }
    getProfit(){
      return "The profil of "${this.title}" book is ${this.price * this.quantity}`;
    }
    toString(){
      return this.title 'books author is'  this.author
    }
    
  }
